<InsertUser>:
    name: "InsertUser"

    userName: name
    comments: comments
    dob: DOB

    Button:
        text: "Cancel"
        pos_hint:{"x":.55, "top":.2}
        on_press:
            root.homepage()
            root.reset()
            root.manager.transition.direction = "right"

    Button:
        text: "Create User"
        pos_hint:{"x":.35, "top":.2}
        on_press:
            root.manager.transition.direction = "left"
            root.createUser()


    Label:
        text:"Create A User"
        pos_hint:{"x":.45, "top":1}
        size_hint: (0.15,0.1)
        font_size:'50sp'
        font_name:'comic'
        color: 13/255,135/255,13/255,1

    Label:
        text:"Name: "
        pos_hint:{"x":.15, "top":.8}
        size_hint: (0.1,0.1)
        font_size:'30sp'
        font_name:'comic'
        color: 13/255,135/255,13/255,1

    Label:
        text:"Comments: "
        pos_hint:{"x":.15, "top":.6}
        size_hint: (0.1,0.1)
        font_size:'30sp'
        font_name:'comic'
        color: 13/255,135/255,13/255,1

    TextInput:
        id:name
